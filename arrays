var arr=[1,23,30,67,80];


arr[1]=56;

arr[3]=70;

var x = arr;

console.log(x);

for( var index= arr.length-1; index>=0; index--){
    if(arr[index]%2==0){
    console.log("index:",index, "value:",[index]);
}
}

output:
[ 1, 56, 30, 70, 80 ]
index: 4 value: [ 4 ]
index: 3 value: [ 3 ]
index: 2 value: [ 2 ]
index: 1 value: [ 1 ]
